<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/logout_btn"
        android:layout_width="345dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="76dp"
        android:text="Log out" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="50dp"
        android:background="?android:attr/listDivider"
        android:visibility="visible" />

    <Button
        android:id="@+id/feedbtn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="11dp"
        android:layout_marginStart="49dp"
        android:background="@drawable/instagram_home_outline_24" />

    <Button
        android:id="@+id/picbtn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignTop="@+id/feedbtn"
        android:layout_centerHorizontal="true"
        android:background="@drawable/instagram_new_post_outline_24" />

    <Button
        android:id="@+id/profilebtn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/feedbtn"
        android:layout_marginEnd="49dp"
        android:background="@drawable/instagram_user_outline_24" />

</RelativeLayout>